{
  "name": "wwltw",
  "version": "0.0.1",
  "scripts": {
    "build": "npm run build:app && npm run copy:jquery && npm run copy:static && npm run copy:bootstrap",
    "build:app": "browserify -e ./app/index.js -o ./dist/app.js",
    "copy:jquery": "cpy 'node_modules/jquery/dist/jquery.min.js' ./dist/scripts/",
    "copy:bootstrap": "cpy node_modules/bootstrap/dist/css/bootstrap.min.css ./dist",
    "copy:static": "cpy './index.html' ./dist/"
  },
  "dependencies": {
    "babel-core": "6.3.x",
    "babel-preset-es2015": "6.3.x",
    "babelify": "7.2.x",
    "bootstrap": "^3.3.7",
    "cpy": "3.4.x",
    "jquery": "^3.1.1",
    "npm-run-all": "1.4.x",
    "sassify": "0.9.x",
    "semantic-ui": "2.1.x"
  },
  "devDependencies": {
    "jasmine": "^2.5.3"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ],
      [
        "sassify",
        {
          "auto-inject": true
        }
      ]
    ]
  }
}
